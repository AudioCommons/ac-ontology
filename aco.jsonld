      },
      {
         "id": "#nodeContent",
         "type": [
            "owl:FunctionalProperty",
            "owl:ObjectProperty"
         ],
         "comment": "Associates a node with the actual content (an audio manifestation).",
         "domain": "#AudioCollectionNode",
         "rdfs:isDefinedBy": {
            "id": "aco"
         },
         "label": "node content",
         "range": "#AudioManifestation",
         "subPropertyOf": "rdf:first"
      },
