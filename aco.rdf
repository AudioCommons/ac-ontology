                    </rdf:Description>
                </owl:onProperty>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    <owl:Class rdf:about="https://w3id.org/ac-ontology/aco#AudioCollectionLastNode">
