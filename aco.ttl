@prefix : <#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix mo: <http://purl.org/ontology/mo/>.
@prefix rd: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix ont: <https://w3id.org/audiocommons/ontology#>.
@prefix n0: <https://w3id.org/audiocommons/>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix terms: <http://purl.org/dc/terms/>.
@prefix v: <http://purl.org/vocab/vann/>.
@prefix n1: <http://xmlns.com/foaf/0.1/>.
@prefix core: <http://purl.org/vocab/frbr/core#>.
@prefix XML: <http://www.w3.org/2001/XMLSchema#>.
@prefix ebu: <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#>.
@prefix cc: <http://web.resource.org/cc/>.
@prefix eve: <http://purl.org/NET/c4dm/event.owl#>.

mo:AnalogSignal rd:subClassOf ont:AnalogSignal.

mo:AudioFile rd:subClassOf ont:AudioFile.

mo:available_as rd:subPropertyOf ont:availableAs.

mo:bitsPerSample rd:subPropertyOf ont:bitsPerSample.

mo:channels rd:subPropertyOf ont:channels.

mo:compiled rd:subPropertyOf ont:compiled.

mo:DigitalSignal rd:subClassOf ont:DigitalSignal.

mo:duration rd:subPropertyOf ont:duration.

mo:encodes rd:subPropertyOf ont:encodes.

mo:genre rd:subPropertyOf ont:audioCategory.

mo:Genre rd:subClassOf ont:AudioCategory.

mo:homepage rd:subPropertyOf ont:homepage.

mo:image rd:subPropertyOf ont:image.

mo:instrument rd:subPropertyOf ont:audioCategory.

mo:Instrument rd:subClassOf ont:AudioCategory.

mo:MusicalExpression rd:subClassOf ont:AudioExpression.

mo:MusicalItem rd:subClassOf ont:AudioItem.

mo:MusicalManifestation rd:subClassOf ont:AudioManifestation.

mo:produced_signal rd:subPropertyOf ont:producedSignal.

mo:publication_of rd:subPropertyOf ont:publicationOf.

mo:published rd:subPropertyOf ont:published.

mo:Record rd:subClassOf ont:AudioCollection.

mo:Recording rd:subClassOf ont:Recording.

mo:recording_of rd:subPropertyOf ont:recordingOf.

mo:release rd:subPropertyOf ont:publishedAudioManifestation.

mo:Release rd:subClassOf ont:AudioCollection.

mo:ReleaseEvent rd:subClassOf ont:AudioPublication.

mo:sample_rate rd:subPropertyOf ont:sampleRate.

mo:Signal rd:subClassOf ont:Signal.

mo:Sound rd:subClassOf ont:Sound.

mo:Track rd:subClassOf ont:AudioClip.

n0:ontology
    a owl:Ontology;
    terms:created "2017/07/27 11:09:00";
    terms:date "2017/07/27 11:09:00";
    terms:description
        """The Audio Commons Ontology provides main concepts and properties for describing audio content, both musical and non-musical, on the Semantic Web.
""";
    terms:title "The Audio Commons Ontology";
    v:preferredNamespacePrefix "ac";
    v:preferredNamespaceUri "https://w3id.org/audiocommons/ontology#";
    owl:versionInfo "Revision: 0.0.0";
    n1:maker [ n1:name "George Fazekas" ], [ n1:name "Miguel Ceriani" ].
ont:AnalogSignal
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Analog signal";
    rd:subClassOf ont:Signal;
    owl:disjointWith ont:DigitalSignal.
ont:audioCategory
    a owl:ObjectProperty;
    rd:isDefinedBy n0:ontology;
    rd:label "is in audio category";
    rd:range ont:AudioCategory.
ont:AudioCategory
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Audio category";
    rd:subClassOf core:Concept.
ont:AudioClip
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Audio clip";
    rd:subClassOf ont:AudioManifestation.
ont:AudioCollection
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Audio collection";
    rd:subClassOf ont:AudioManifestation.
ont:AudioCollectionFirstNode
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "First node";
    rd:subClassOf ont:AudioCollectionNode.
ont:AudioCollectionLastNode
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Last node";
    rd:subClassOf ont:AudioCollectionNode.
ont:AudioCollectionNode
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Node";
    rd:subClassOf
        rdf:List,
            [
                a owl:Restriction;
                owl:allValuesFrom ont:AudioCollectionNode;
                owl:onProperty rdf:rest
            ],
            [
                a owl:Restriction;
                owl:allValuesFrom ont:AudioManifestation;
                owl:onProperty rdf:first
            ],
            [
                a owl:Restriction;
                owl:onClass ont:AudioCollectionNode;
                owl:onProperty rdf:rest;
                owl:qualifiedCardinality "1"^^XML:nonNegativeInteger
            ],
            [
                a owl:Restriction;
                owl:onClass ont:AudioManifestation;
                owl:onProperty rdf:first;
                owl:qualifiedCardinality "1"^^XML:nonNegativeInteger
            ],
            [
                a owl:Restriction;
                owl:onClass ont:AudioManifestation;
                owl:onProperty ont:nodeContent;
                owl:qualifiedCardinality "1"^^XML:nonNegativeInteger
            ];
    owl:disjointUnionOf
            [
                rdf:first ont:AudioCollectionFirstNode;
                rdf:rest
                [ rdf:first ont:AudioCollectionNonFirstNode; rdf:rest rdf:nil ]
            ],
            [
                rdf:first ont:AudioCollectionLastNode;
                rdf:rest
                [ rdf:first ont:AudioCollectionNonLastNode; rdf:rest rdf:nil ]
            ].
ont:AudioCollectionNonFirstNode
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Any node but the first";
    rd:subClassOf ont:AudioCollectionNode.
ont:AudioCollectionNonLastNode
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Any node but the last";
    rd:subClassOf ont:AudioCollectionNode.
ont:AudioExpression
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Audio expression";
    rd:subClassOf core:Expression.
ont:AudioFile
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Audio file";
    rd:subClassOf ebu:MediaResource, ont:AudioItem.
ont:AudioItem
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Audio item";
    rd:subClassOf core:Item.
ont:AudioManifestation
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Audio manifestation";
    rd:subClassOf core:Manifestation, cc:Work.
ont:AudioPublication
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Audio publication";
    rd:subClassOf eve:Event.
ont:availableAs
    a owl:ObjectProperty;
    rd:domain ont:AudioManifestation;
    rd:isDefinedBy n0:ontology;
    rd:label "available as";
    rd:range ont:AudioItem.
ont:bitsPerSample
    a owl:DatatypeProperty, owl:FunctionalProperty;
    rd:domain ont:DigitalSignal;
    rd:isDefinedBy n0:ontology;
    rd:label "bits per sample";
    rd:range XML:int.
ont:channels
    a owl:DatatypeProperty, owl:FunctionalProperty;
    rd:domain ont:DigitalSignal;
    rd:isDefinedBy n0:ontology;
    rd:label "channels";
    rd:range XML:int.
ont:compiled
    a owl:ObjectProperty;
    rd:domain n1:Agent;
    rd:isDefinedBy n0:ontology;
    rd:label "compiled";
    rd:range ont:AudioManifestation.
ont:DigitalSignal
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Digital signal";
    rd:subClassOf ont:Signal;
    owl:disjointWith ont:AnalogSignal.
ont:duration
    a owl:DatatypeProperty, owl:FunctionalProperty;
    rd:domain [ a owl:Class; owl:unionOf [] ];
    rd:isDefinedBy n0:ontology;
    rd:label "duration";
    rd:range XML:int.
ont:encodes
    a owl:ObjectProperty;
    rd:domain ont:AudioItem;
    rd:isDefinedBy n0:ontology;
    rd:label "encodes";
    rd:range ont:DigitalSignal.
ont:firstNode
    a owl:FunctionalProperty, owl:ObjectProperty;
    rd:domain ont:AudioCollection;
    rd:isDefinedBy n0:ontology;
    rd:label "first node";
    rd:range ont:AudioCollectionFirstNode;
    rd:subPropertyOf ont:memberNode.
ont:homepage
    a owl:ObjectProperty;
    rd:isDefinedBy n0:ontology;
    rd:label "homepage";
    rd:range n1:Document;
    rd:subPropertyOf n1:isPrimaryTopicOf.
ont:image
    a owl:ObjectProperty;
    rd:isDefinedBy n0:ontology;
    rd:label "image";
    rd:range n1:Image;
    rd:subPropertyOf n1:depiction.
ont:lastNode
    a owl:FunctionalProperty, owl:ObjectProperty;
    rd:domain ont:AudioCollection;
    rd:isDefinedBy n0:ontology;
    rd:label "last node";
    rd:range ont:AudioCollectionLastNode;
    rd:subPropertyOf ont:memberNode.
ont:memberNode
    a owl:ObjectProperty;
    rd:domain ont:AudioCollection;
    rd:isDefinedBy n0:ontology;
    rd:label "member node";
    rd:range ont:AudioCollectionNode.
ont:nextNode
    a owl:FunctionalProperty, owl:ObjectProperty;
    rd:domain ont:AudioCollectionNonLastNode;
    rd:isDefinedBy n0:ontology;
    rd:label "next node";
    rd:range ont:AudioCollectionNonFirstNode;
    rd:subPropertyOf rdf:rest.
ont:nodeContent
    a owl:FunctionalProperty, owl:ObjectProperty;
    rd:domain ont:AudioCollectionNode;
    rd:isDefinedBy n0:ontology;
    rd:label "node content";
    rd:range ont:AudioManifestation;
    rd:subPropertyOf rdf:first.
ont:nodeIndex
    a owl:DatatypeProperty, owl:FunctionalProperty;
    rd:domain ont:AudioCollectionNode;
    rd:isDefinedBy n0:ontology;
    rd:label "node index";
    rd:range XML:int.
ont:previousNode
    a owl:FunctionalProperty, owl:ObjectProperty;
    rd:domain ont:AudioCollectionNonFirstNode;
    rd:isDefinedBy n0:ontology;
    rd:label "previous node";
    rd:range ont:AudioCollectionNonLastNode.
ont:producedSignal
    a owl:ObjectProperty;
    rd:domain ont:SignalProduction;
    rd:isDefinedBy n0:ontology;
    rd:label "produced signal";
    rd:range ont:Signal.
ont:productionFrom
    a owl:ObjectProperty;
    rd:domain ont:SignalProduction;
    rd:isDefinedBy n0:ontology;
    rd:label "production from";
    rd:range ont:Sound.
ont:publicationOf
    a owl:ObjectProperty;
    rd:domain ont:AudioManifestation;
    rd:isDefinedBy n0:ontology;
    rd:label "publication of";
    rd:range ont:AudioExpression.
ont:published
    a owl:ObjectProperty;
    rd:domain n1:Agent;
    rd:isDefinedBy n0:ontology;
    rd:label "published";
    rd:range ont:AudioManifestation.
ont:publishedAudioManifestation
    a owl:ObjectProperty;
    rd:domain ont:AudioPublication;
    rd:isDefinedBy n0:ontology;
    rd:label "published audio manifestation";
    rd:range ont:AudioManifestation.
ont:Recording
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Recording";
    rd:subClassOf ont:SignalProduction.
ont:recordingOf
    a owl:ObjectProperty;
    rd:domain ont:Recording;
    rd:isDefinedBy n0:ontology;
    rd:label "recording of";
    rd:range ont:Sound;
    rd:subPropertyOf ont:productionFrom.
ont:sampleRate
    a owl:DatatypeProperty, owl:FunctionalProperty;
    rd:domain ont:DigitalSignal;
    rd:isDefinedBy n0:ontology;
    rd:label "sample rate";
    rd:range XML:float.
ont:Signal
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Signal";
    rd:subClassOf ont:AudioExpression.
ont:SignalProduction
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Signal production";
    rd:subClassOf eve:Event.
ont:Sound
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Physical sound";
    rd:subClassOf ont:AudioExpression.
ont:Syntesis
    a owl:Class;
    rd:isDefinedBy n0:ontology;
    rd:label "Syntesis";
    rd:subClassOf ont:SignalProduction.
