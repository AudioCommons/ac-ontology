@prefix ac:      <http://audiocommons.org/ns/audiocommons#> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix event:   <http://purl.org/NET/c4dm/event.owl#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix frbr:    <http://purl.org/vocab/frbr/core#> .
@prefix mo:      <http://purl.org/ontology/mo/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix time:    <http://www.w3.org/2006/time#> .
@prefix vann:    <http://purl.org/vocab/vann/> .
@prefix vs:      <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix wot:     <http://xmlns.com/wot/0.1/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

ac:
    a owl:Ontology;
    dcterms:created "2017/07/27 11:09:00";
    dcterms:date "2017/07/27 11:09:00";
    dcterms:description """
        The Audio Commons Ontology provides main concepts and properties for
        describing audio content, both musical and non-musical, on the Semantic Web.""";
    dcterms:title "The Audio Commons Ontology";
    owl:versionInfo "Revision: 0.0.0";
    foaf:maker [foaf:name "Miguel Ceriani"], [foaf:name "George Fazekas"].

ac:AudioExpression
    a owl:Class;
    rdfs:isDefinedBy ac:;
    rdfs:label "Audio expression";
    rdfs:subClassOf frbr:Expression.
mo:MusicalExpression rdfs:subClassOf ac:AudioExpression.

ac:AudioManifestation
    a owl:Class;
    rdfs:isDefinedBy ac:;
    rdfs:label "Audio manifestation";
    rdfs:subClassOf frbr:Manifestation, cc:Work.
mo:MusicalManifestation rdfs:subClassOf ac:AudioManifestation.

ac:AudioItem
    a owl:Class;
    rdfs:isDefinedBy ac:;
    rdfs:label "Audio item";
    rdfs:subClassOf frbr:Item.
mo:MusicalItem rdfs:subClassOf ac:AudioItem.

ac:AudioCategory
    a owl:Class;
    rdfs:isDefinedBy ac:;
    rdfs:label "Audio category";
    rdfs:subClassOf frbr:Concept.
mo:Genre rdfs:subClassOf ac:AudioCategory.
mo:Instrument rdfs:subClassOf ac:AudioCategory.
