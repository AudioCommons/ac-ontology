@prefix ac:      <#> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix event:   <http://purl.org/NET/c4dm/event.owl#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix frbr:    <http://purl.org/vocab/frbr/core#> .
@prefix mo:      <http://purl.org/ontology/mo/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix time:    <http://www.w3.org/2006/time#> .
@prefix vann:    <http://purl.org/vocab/vann/> .
@prefix vs:      <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix wot:     <http://xmlns.com/wot/0.1/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

ac:AudioCollection
    a owl:Class;
    rdfs:isDefinedBy ac:;
    rdfs:label "Audio collection";
    rdfs:subClassOf ac:AudioManifestation.
mo:Record rdfs:subClassOf ac:AudioCollection.
mo:Release rdfs:subClassOf ac:AudioCollection.

ac:memberNode
    a owl:ObjectProperty;
    rdfs:isDefinedBy ac:;
    rdfs:label "member node";
    rdfs:domain ac:AudioCollection;
    rdfs:range: ac:AudioCollectionNode.

ac:firstNode
    a owl:ObjectProperty, owl:FunctionalProperty;
    rdfs:isDefinedBy ac:;
    rdfs:label "first node";
    rdfs:subPropertyOf ac:memberNode;
    rdfs:domain ac:AudioCollection;
    rdfs:range: ac:AudioCollectionFirstNode.

ac:lastNode
    a owl:ObjectProperty, owl:FunctionalProperty;
    rdfs:isDefinedBy ac:;
    rdfs:label "first node";
    rdfs:subPropertyOf ac:memberNode;
    rdfs:domain ac:AudioCollection;
    rdfs:range: ac:AudioCollectionLastNode.

ac:AudioCollectionNode
    a owl:Class;
    rdfs:isDefinedBy ac:;
    rdfs:label "Audio collection node";
    rdfs:subClassOf rdf:List;
    rdfs:subClassOf [
        rdf:type owl:Restriction;
        owl:onProperty rdf:rest;
        owl:onClass rdf:List;
        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ].
    owl:disjointUnionOf
        (ac:AudioCollectionFirstNode ac:AudioCollectionNonFirstNode),
        (ac:AudioCollectionLastNode ac:AudioCollectionNonLastNode).

ac:AudioCollectionFirstNode
    a owl:Class;
    rdfs:isDefinedBy ac:;
    rdfs:label "First node of an audio collection";
    rdfs:subClassOf ac:AudioCollectionNode.

ac:AudioCollectionLastNode
    a owl:Class;
    rdfs:isDefinedBy ac:;
    rdfs:label "Last node of an audio collection";
    owl:intersectionOf(
        ac:AudioCollectionNode
        [ rdf:type owl:Restriction;
          owl:onProperty rdf:rest;
          owl:hasValue rdf:nil ] ).

rdf:nil a [ rdf:type owl:Restriction;
            owl:onProperty rdf:rest;
            owl:onClass rdf:List;
            owl:qualifiedCardinality "0"^^xsd:nonNegativeInteger ].

ac:AudioCollectionNonFirstNode
    a owl:Class;
    rdfs:isDefinedBy ac:;
    rdfs:label "Any node of an audio collection but the first";
    rdfs:subClassOf ac:AudioCollectionNode;
    owl:equivalentClass [
        rdf:type owl:Restriction;
        owl:onProperty ac:previousNode;
        owl:someValuesFrom ac:AudioCollectionNode ].

ac:AudioCollectionNonLastNode
    a owl:Class;
    rdfs:isDefinedBy ac:;
    rdfs:label "Any node of an audio collection but the last";
    rdfs:subClassOf ac:AudioCollectionNode;
    owl:equivalentClass [
        rdf:type owl:Restriction;
        owl:onProperty ac:nextNode;
        owl:someValuesFrom ac:AudioCollectionNode ].

[ rdf:type owl:Restriction;
  owl:onProperty rdf:rest;
  owl:someValuesFrom rdf:List ]
      owl:disjointUnionOf (
          ac:AudioCollectionNonLastNode
          _:otherList ).

ac:nodeContent
    a owl:ObjectProperty, owl:FunctionalProperty;
    rdfs:isDefinedBy ac:;
    rdfs:label "node content";
    rdfs:subPropertyOf rdf:first;
    rdfs:domain ac:AudioCollectionNode;
    rdfs:range: ac:AudioManifestation.

ac:nodeIndex
    a owl:DatatypeProperty;
    rdfs:isDefinedBy ac:;
    rdfs:label "node index";
    rdfs:domain ac:AudioCollectionNode;
    rdfs:range: xsd:int.

ac:nextNode
    a owl:ObjectProperty, owl:FunctionalProperty;
    rdfs:isDefinedBy ac:;
    rdfs:label "next node";
    rdfs:subPropertyOf rdf:rest;
    owl:inverseOf ac:previousNode;
    rdfs:domain ac:AudioCollectionNonLastNode;
    rdfs:range: ac:AudioCollectionNonFirstNode.

ac:previousNode
    a owl:ObjectProperty, owl:FunctionalProperty;
    rdfs:isDefinedBy ac:;
    rdfs:label "previous node";
    owl:inverseOf ac:nextNode;
    rdfs:domain ac:AudioCollectionNonFirstNode;
    rdfs:range: ac:AudioCollectionNonLastNode.
